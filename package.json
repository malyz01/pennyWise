{
  "name": "pennywise",
  "version": "0.0.1",
  "description": "Pennywise money saver app",
  "repository": "https://github.com/malyz01/pennyWise.git",
  "main": "server/index.js",
  "author": "Malyz",
  "license": "ISC",
  "scripts": {
    "client": "webpack-dev-server --hot --inline",
    "dev:h": "run-p client dev:server",
    "dev": "run-p dev:client dev:server",
    "dev:client": "webpack --watch",
    "dev:server": "nodemon server",
    "start": "npm run server",
    "build": "webpack",
    "server": "node server",
    "test": "jest --watchAll",
    "knex": "knex --knexfile server/db/knexfile.js",
    "postinstall": "run-s db:reset build",
    "db:reset": "run-s db:delete db:migrate db:seed",
    "db:refresh": "run-s db:rollback db:migrate db:seed",
    "db:delete": "rm -f server/db/dev.sqlite3",
    "db:rollback": "npm run knex migrate:rollback",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
       ["@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
      "recharts"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "camelcase-keys": "^6.2.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "formik": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.21.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.3",
    "react-reveal": "^1.2.2",
    "react-scroll": "^1.7.16",
    "react-slick": "^0.25.2",
    "semantic-ui-react": "^0.88.2",
    "snakecase-keys": "^3.2.0",
    "sqlite3": "^4.2.0",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-recharts": "^1.2.1",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^6.0.0",
    "jest": "^25.2.7",
    "nodemon": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
